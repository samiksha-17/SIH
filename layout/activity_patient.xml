<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kim2"
    tools:context=".patientActivity">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="16dp"
        android:text="PATIENT REGISTRATION"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/textView61"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="387dp"
        android:layout_height="37dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="16dp"
        android:drawableLeft="@drawable/ic_baseline_date_range_24"
        android:paddingTop="0sp"
        android:paddingBottom="7sp"
        android:text="Date Of Birth :"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/edt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/edt"
        android:layout_width="407dp"
        android:layout_height="33dp"
        android:layout_marginBottom="6dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="8dp"
        android:focusable="false"
        android:hint="Select Date"
        android:inputType="none"
        android:paddingBottom="7sp"
        android:paddingLeft="20sp"
        android:paddingTop="0sp"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView61" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="392dp"
        android:layout_height="37dp"
        android:layout_marginBottom="7dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/textborder"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:paddingBottom="7sp"
        android:paddingLeft="25sp"
        android:paddingTop="0sp"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/countrycode"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edt" />

    <TextView

        android:id="@+id/countrycode"
        android:layout_width="51dp"
        android:layout_height="43dp"
        android:layout_marginEnd="30dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/textborder"
        android:text="+91"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/editTextPhone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress2" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="255dp"
        android:layout_height="41dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="51dp"
        android:background="@drawable/textborder"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/countrycode"
        app:layout_constraintTop_toTopOf="@+id/editTextTextEmailAddress2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="84dp"
        android:layout_height="37dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="294dp"
        android:layout_marginBottom="19dp"
        android:text="Gender"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/countrycode" />

    <RadioButton
        android:id="@+id/male_rb"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="108dp"
        android:layout_marginEnd="30dp"
        android:layout_marginStart="4dp"
        android:text="Male"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress"
        app:layout_constraintEnd_toStartOf="@+id/radioButton3"
        app:layout_constraintStart_toStartOf="@+id/imageView" />

    <RadioButton
        android:id="@+id/female_rb"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:text="Female"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/textView69"
        app:layout_constraintEnd_toStartOf="@+id/male_rb"
        app:layout_constraintHorizontal_bias="0.301"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <RadioButton
        android:id="@+id/radioButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="260dp"
        android:text="Others"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView68"
        tools:ignore="UnknownId" />

    <EditText
        android:id="@+id/editTextTextPostalAddress"
        android:layout_width="393dp"
        android:layout_height="37dp"
        android:layout_marginBottom="9dp"
        android:layout_marginEnd="9dp"
        android:layout_marginStart="9dp"
        android:background="@drawable/textborder"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPostalAddress"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner4" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="67dp"
        android:layout_height="46dp"
        android:layout_marginBottom="3dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="73dp"
        app:layout_constraintBottom_toTopOf="@+id/male_rb"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
        app:layout_constraintVertical_bias="0.73"
        app:srcCompat="@drawable/boy" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="67dp"
        android:layout_height="51dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/female_rb"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/girl" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="74dp"
        android:layout_height="52dp"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="96dp"
        app:layout_constraintBottom_toTopOf="@+id/radioButton3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
        app:layout_constraintVertical_bias="0.317"
        app:srcCompat="@drawable/others" />

    <TextView
        android:id="@+id/textView68"
        android:layout_width="113dp"
        android:layout_height="34dp"
        android:layout_marginEnd="56dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"

        android:paddingBottom="6sp"
        android:text="States"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/spinner4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView69"
        app:layout_constraintTop_toBottomOf="@+id/radioButton3" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="152dp"
        android:layout_height="37dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spinner5"
        app:layout_constraintTop_toBottomOf="@+id/textView68" />


    <TextView
        android:id="@+id/textView69"
        android:layout_width="112dp"
        android:layout_height="34dp"
        android:layout_marginEnd="98dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="8dp"

        android:paddingBottom="7sp"
        android:paddingTop="0sp"
        android:text="Districts"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/spinner5"
        app:layout_constraintEnd_toStartOf="@+id/textView68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/female_rb" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="152dp"
        android:layout_height="37dp"
        android:layout_marginBottom="4dp"
        android:layout_marginEnd="39dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress"
        app:layout_constraintEnd_toStartOf="@+id/spinner4"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="172dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="15dp"
        android:text="Register"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:onClick="activity_Hospital"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPostalAddress"
       />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginEnd="74dp"
        android:layout_marginBottom="15dp"
        android:text="Already Have Account?SignIn"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register" />

</androidx.constraintlayout.widget.ConstraintLayout>